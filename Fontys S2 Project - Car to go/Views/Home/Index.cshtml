@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_LayoutCustom.cshtml";
}

@if (TempData["Update"] != null)
{
    <strong class="alert alert-warning">@TempData["Update"]</strong>
    <br />
    <br />
}
@if (TempData["Delete"] != null)
{
    <strong class="alert alert-danger">@TempData["Delete"]</strong>
    <br />
    <br />
}
@if (TempData["LoggedIn"] != null)
{
    <strong class="alert alert-success">@TempData["LoggedIn"]</strong>
    <br />
    <br />
}


@Html.Partial("_partialIndex")